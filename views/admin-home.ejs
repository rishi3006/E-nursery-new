</head>
<body>

<!-- header section starts  -->

<header>

    <div class="header-1">

        <a href="/" class="logo"><i class="fas fa-shopping-basket"></i><span style="color:#27ae60;"> E-</span>nursery</a>
    </div>

    <div class="header-2">

        <div id="menu-bar" class="fas fa-bars"></div>

        <nav class="navbar">
            <!-- <a href="/">home</a> -->
            <!-- <a href="#product">product</a> -->
            <!-- <a href="/team">Team</a> -->
        </nav>

        <div class="icons">
            <a href="/"><i class="fa-solid fa-right-from-bracket"></i></a>
        </div>

    </div>

</header>

<!-- header section ends -->

<!-- product section starts  -->

<section class="product" id="product">

    <h1 class="heading"><span>Admin</span></h1>

    <div class="box-container">
        
    <%  plants.forEach(function(plant){ %>

        <!-- <form method=""> -->

        <!-- </form> -->
        <form action="/remove-plant" method="post">
            <div id=<%=plant.name%> class="box">
                <img src= <%=plant.image%> alt="">
                <h3><%=plant.name%></h3>
                
                <h2>price: Rs.<%=plant.price%></h2>
                <div class="btn-container">
                    <a href="/viewPlant/<%=plant.name%>" class="btn"><i class="fas fa-solid fa-eye"></i></a>
                    <!-- <form action="/remove-plant" method="post"> -->
                    <button  name="removePlantId" value="<%= plant._id %>" class="btn" type="submit"><i class="fa-solid fa-trash-can"></i></button>
                    <!-- </form> -->
                    <!-- <a href="" class="btn">Modify</a> -->
                </div>
                
                </form>
            </div>
                <% }) %>
        <!-- <div class="box-container"> -->
            <!-- <div class="box"> -->
                <!-- <a href=""> -->
            <a class="plus-sign" href="/add-plant"><img src="data:image/png;base64," alt="add-plant"></a>
            <!-- </div> -->
        <!-- </div> -->
    
</section>

<!-- product section ends -->
<!-- footer section starts  -->

<section class="footer">

    <h1 class="credit"> Developed by <span> e-nursery </span> | all rights reserved! </h1>

</section>

